{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": false,
    "jsx": "react-jsx",
    "declaration": true,
    "declarationDir": "lib",
    "baseUrl": "src"
  },
  "include": [
    "src"
  ],
  "paths": {
    "components/*": [
      "./components/*"
    ],
    "interfaces/*": [
      "./interfaces/*"
    ],
    "utils/*": [
      "./utils/*"
    ],
    "scss/*": [
      "./scss/*"
    ],
    "fonts/*": [
      "./fonts/*"
    ],
    "src/*": [
      "./*"
    ],
    "assets/*": [
      "./assets/*"
    ]
  }
}
