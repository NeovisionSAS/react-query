{
  "name": "@neovision/react-query",
  "version": "1.3.2",
  "private": true,
  "author": {
    "email": "alan.blanchet@neovision.fr",
    "name": "Alan Blanchet"
  },
  "scripts": {
    "start": "webpack serve --config ./env/webpack.dev.js --open",
    "lib": "tsconfig-replace-paths --project tsconfig.json && webpack --config ./env/webpack.lib.js",
    "patch": "npm version --no-git-tag-version patch",
    "minor": "npm version --no-git-tag-version minor",
    "major": "npm version --no-git-tag-version major",
    "amend": "npm run lib && git add . && git commit --amend && git push -f",
    "postversion": "npm run lib && git add . && git commit && git tag v$(npx -c 'node -p \"process.env.npm_package_version\"') && git push && git push --tags"
  },
  "main": "./src/index.tsx",
  "module": "./lib/module.js",
  "types": "./lib/module.d.ts",
  "files": [
    "lib/"
  ],
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.0",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.4.0",
    "mini-css-extract-plugin": "^2.6.0",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tsconfig-replace-paths": "0.0.11",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  }
}
